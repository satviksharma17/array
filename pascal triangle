import java.util.*; 
public class MyClass {
   void pascal(int rows)
   {
       List<List<Integer>> res=new ArrayList<List<Integer>>();
       List<Integer> row=,pre=null;
       for(int i=0;i<rows;i++)
       { row=new ArrayList<Integer>();
           for(int j=0;j<=i;j++)
           if(j==0 || j==i)
           row.add(1);
           else
           row.add(pre.get(j-1)+pre.get(j));
           pre=row;
           res.add(row);
       }
   }
      
}
